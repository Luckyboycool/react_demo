{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build:none": "COMPRESS=none umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@types/echarts": "^4.8.3",
    "@types/js-cookie": "^2.2.6",
    "@types/react-highlight-words": "^0.16.1",
    "@types/styled-components": "^5.1.3",
    "@types/zrender": "^4.0.0",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.19",
    "axios": ">=0.21.1",
    "echarts": "^4.9.0",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.7",
    "mathjs": "^7.5.1",
    "moment": "^2.27.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-color": "^2.18.1",
    "react-dom": "^16.12.0",
    "react-highlight-words": "^0.16.0",
    "styled-components": "^5.2.0",
    "umi": "^3.2.19",
    "yorkie": "^2.0.0",
    "zrender": "^4.3.2"
  }
}
